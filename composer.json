{
  "name": "zgabievi/kendo-grid-state",
  "description": "Helper package to handle kendo grid state in Laravel",
  "license": "MIT",
  "keywords": [
    "kendo",
    " grid",
    " state",
    " filter",
    " group",
    " skip",
    " sort",
    " take"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Zura Gabievi",
      "email": "zura.gabievi@gmail.com"
    },
    {
      "name": "Greg Heafield",
      "email": "greg@binaryfold4.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/database": ">=5.6",
    "illuminate/http": ">=5.6",
    "illuminate/support": ">=5.6"
  },
  "require-dev": {
    "illuminate/routing": "^5.6",
    "orchestra/testbench": "~3.6.0",
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "Zgabievi\\KendoGridState\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zgabievi\\KendoGridState\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Zgabievi\\KendoGridState\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
